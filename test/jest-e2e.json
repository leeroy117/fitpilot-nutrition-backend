{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "test/.*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": "tsconfig.test.json"
    }]
  },
  "moduleNameMapper": {
    "^@generated/(.*)$": "<rootDir>/generated/$1",
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}
